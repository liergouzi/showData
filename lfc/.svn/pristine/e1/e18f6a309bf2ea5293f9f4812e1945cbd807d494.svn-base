<div class="portlet box red portlet-mg0 ">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-gift"></i>督导员详情页</div>
        <div class="tools">
            <i class="fa fa-close" ng-click="cancel()"></i>
        </div>
    </div>
    <div class="portlet-body form">
        <div class="form-horizontal">
            <div class="form-body">
                <div class="head text-center">
                    <img src="{{params.user_avatar|photoFilter}}" width="120" height="120">
                </div>
                <div class="left-right clearfix">
                    <div class="pull-left">
                        <div>督导员:<div class="inline-block" style="vertical-align: top" ><p>{{params.user_name}}</p></div></div>
                        <div>是否在岗:<div class="inline-block" style="vertical-align: top" ><p>{{params.status|statusFilter}}</p></div></div>
                        <div>手机号:<div class="inline-block" style="vertical-align: top" ><p>{{params.user_mobile}}</p></div></div>
                    </div>
                    <div class="pull-right">
                        <div>性别:<div class="inline-block" style="vertical-align: top" ><p>{{params.user_gender|sexFilter}}</p></div></div>
                        <div>分支机构:<div class="inline-block" style="vertical-align: top" ><p>{{params.company_name}}</p></div></div>
                        <div>身份证号:<div class="inline-block" style="vertical-align: top" ><p>{{params.user_identityNum}}</p></div></div>
                    </div>
                    <div style="width: 800px;">管理范围:<div class="inline-block" style="vertical-align: top" ><p ng-repeat="li in params.domain">{{li}}</p></div></div>
                </div>
            </div>
        </div>
    </div>
</div>

